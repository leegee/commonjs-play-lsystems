{
  "name": "commonjs-play-lsystems",
  "version": "1.0.0",
  "description": "L-system tools",
  "main": "app/desktop.html",
  "window": {
    "toolbar": true,
    "width": 800,
    "height": 500,
    "position": "center"
  },
  "scripts": {
    "miditest": "grunt mochaTest",
    "test": "mocha spec/test.js",
    "oldtest": "browserify spec/tests.js -o build/tests-bundle-2d-gui.js --debug --verbose; node build/tests-bundle-2d.js",
    "build2d": "browserify app/2d.js -o build/bundle-2d.js",
    "build3d": "browserify app/3d.js -o build/bundle-3d.js",
    "buildgui": "browserify app/gui.js -o build/bundle-gui.js",
    "watch2d": "watchify app/2d.js -o build/bundle-2d.js --debug --verbose",
    "watch3d": "watchify app/3d.js -o build/bundle-3d.js --debug --verbose",
    "watchgui": "watchify app/gui.js -o build/bundle-gui.js --debug --verbose"
  },
  "keywords": [
    "l-system",
    "lsystem",
    "lsys",
    "lindenmeyer",
    "system"
  ],
  "author": "lee goddard",
  "license": "ISC",
  "devDependencies": {
    "Log4js": "*",
    "chai": "*",
    "es6ify": "^1.4.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.1",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-mocha-phantomjs": "^2.0.0",
    "grunt-mocha-test": "^0.12.7",
    "htmlparser": "latest",
    "jsdom": "^7.0.1",
    "jsmidgen": "*",
    "mocha": "*",
    "mocha-jsdom": "^1.0.0",
    "mocha-phantomjs": "^4.0.1",
    "node-dom": "^0.1.0",
    "node-phantom": "^0.2.5",
    "phantomjs": "^1.9.18"
  },
  "dependencies": {
    "amdefine": "^1.0.0",
    "imagemagick": "*"
  },
  "repository": "git@github.com:leegee/commonjs-play-lsystems.git"
}
