{
	"name": "commonjs-play-lsystems",
	"version": "1.0.1",
	"description": "L-system Tools For Sound",
	"main": "eg/2d.html",
	"scripts": {
		"debug": "webpack --display-error-details --config webpack.config.js",
		"start": "tiny-server",
		"webpack": "webpack",
		"test": "browserify spec/tests.js -o build/tests-bundle-2d.js --debug --verbose; node build/tests-bundle-2d.js",
		"xxx-test3d": "browserify spec/tests.js -o build/tests-bundle-3d.js --debug --verbose; node build/tests-bundle-3d.js",
		"build2d": "browserify app/2d.js browserify app/2d.js -o build/bundle-2d.js",
		"watch2d": "watchify app/2d.js -o build/bundle-2d.js --debug --verbose",
		"watch3d": "watchify app/3d.js -o build/bundle-3d.js --debug --verbose",
		"build3d": "browserify app/3d.js browserify app/3d.js -o build/bundle-3d.js"
	},
	"keywords": [
		"l-system",
		"lsystem",
		"lsys",
		"lindenmeyer",
		"system",
		"midi",
		"sound"
	],
