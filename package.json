	"license": "ISC",
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-polyfill": "^6.26.0",
		"babel-preset-stage-2": "^6.24.1",
		"webpack": "^3.10.0"
	},
	"repository": "git@github.com:leegee/commonjs-play-lsystems.git",
	"dependencies": {}
}